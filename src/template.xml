<div class="fancyoverlay fancyoverlay-hugeinc <%= cssclasses %>">
  <button type="button" class="fancyoverlay-close">Close</button>
  <header class="fancyoverlay-header">
    <% if (title) { %><h1><%= title %></h1><% } %>
    <%= header %>
  </header>
  <div class="fancyoverlay-container">
    <%= prepend %>
    <%= content %>
  </div>
  <% if (buttons) { %>
    <%= buttons %>
  <% } %>
</div>
